          status:
            properties:
              atProvider:
                properties:
                  policy:
                    description: Policy contains the rendered policy in JSON format
                      as it's applied in Backblaze B2.
                    type: string
                  policyName:
                    description: PolicyName shows the actual name of the policy in
                      Backblaze B2.
                    type: string
                type: object
              conditions:
                description: Conditions of the resource.
                items:
                  description: A Condition that may apply to a resource.
