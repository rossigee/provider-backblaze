                properties:
                  name:
                    description: Name of the secret.
                    type: string
                  namespace:
                    description: Namespace of the secret.
                    type: string
                required:
                - name
                - namespace
                type: object
            type: object
          status:
            properties:
              atProvider:
                properties:
                  applicationKeyID:
                    description: ApplicationKeyID is the unique identifier for the
                      application key.
                    type: string
                  bucketID:
                    description: BucketID shows which bucket this key is restricted
                      to (if any).
                    type: string
                  capabilities:
                    description: Capabilities lists the operations this key can perform.
                    items:
                      type: string
                    type: array
                  expirationTimestamp:
